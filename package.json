{
  "name": "Cristian Gutierrez wep page",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Web site with Cristian Gutierrez experiences resume",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristiangutierrezdev/cristiangutierrezdev.github.io"
  },
  "keywords": [
    "Resume",
    "Full Stack Developer",
    "Cristian Gutierrez",
    "Experience"
  ],
  "author": {
    "name": "Cristian Gutierrez",
    "email": "cristiandisenadorvisual@gmail.com",
    "url": "https://github.com/cristiangutierrezdev/"
  },
  "license": "MIT",
  "homepage": "https://cristiangutierrezdev.github.io/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npx prettier-eslint src/**/*.{ts,html} --write",
    "lint:css": "npx stylelint src/**/*.{css,sass}",
    "lint:css-fix": "npx stylelint src/**/*.{css,sass} --fix",
    "prepare": "husky install",
    "commit": "cz"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "npm run lint"
    ],
    "src/**/*.{css,sass}": [
      "npm run lint:css"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
